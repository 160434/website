%table.dimensions
  %tr
    - each dimension in ['Utility', 'Design', 'Innovation', 'Completeness']
      %td
        %label{ 'for': dimension.toLowerCase() }
          %span.tip= dimension === 'Utility' ? 'Utility (?)' : dimension
        %input{ type: 'hidden', id: dimension.toLowerCase(), name: dimension.toLowerCase(), value: vote[dimension.toLowerCase()] || 0 }
        .stars
          - for (var val = 1; val <= 5; val++)
            != '<span class="star" data-value="' + val + '"><img class="empty" src="/images/empty.png"/><img class="filled" src="/images/filled.png"/></span>'
